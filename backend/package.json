    "genkit": "^1.27.0",
    "genkitx-ollama": "^1.27.0",
    "multer": "^2.0.0-rc.4",
    "pdf-parse": "1.1.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.12",
    "@types/pdf-parse": "^1.1.5",
    "@types/supertest": "^6.0.3",
    "pm2": "^6.0.14",
    "supertest": "^7.1.4",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
